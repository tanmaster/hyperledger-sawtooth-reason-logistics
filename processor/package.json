{
  "name": "sawtooth-reason-supply-processor",
  "version": "0.1.0",
  "scripts": {
    "start": "bsb -make-world && node src/web/App.bs.js",
    "dev": "bsb -make-world && razzle start",
    "dev-razzle": "bsb -make-world && razzle start",
    "dev-bsb": "bsb -make-world -w",
    "node-docker": "bsb -make-world && node src/nodeIndex.js",
    "xoTester": "bsb -make-world && node src/sdkIndex.js",
    "clean": "bsb -clean-world && rm -rf lib build && find . -type f -name '*.bs.js' -delete",
    "build": "bsb -make-world && razzle build",
    "test": "razzle test --env=jsdom"
  },
  "dependencies": {
    "express": "^4.17.1",
    "protobufjs": "6.8.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reason-react": "^0.7.0",
    "sawtooth-sdk": "1.0.5"
  },
  "devDependencies": {
    "bs-platform": "5.0.6",
    "concurrently": "^4.0.1",
    "razzle": "^3.0.0"
  }
}
