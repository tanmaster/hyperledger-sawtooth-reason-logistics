{
  "name": "hs-reason",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently \"npm run js-watch\" \"npm run bsb-watch\"",
    "dev": "bsb -make-world && razzle start",
    "clean": "bsb -clean-world && rm -rf lib build && find . -type f -name '*.bs.js' -delete",
    "build": "bsb -make-world && razzle build",
    "test": "razzle test --env=jsdom"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reason-react": "^0.7.0"
  },
  "devDependencies": {
    "bs-platform": "5.0.6",
    "concurrently": "^4.0.1",
    "razzle": "^3.0.0"
  }
}
