{
  "name": "hyperledger-sawtooth-reason-logistics-client",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors","-bs-g"],
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "bs-dependencies": [
    "reason-react",
    "reason-apollo",
    "bs-css",
    "@glennsl/rebase",
    "@glennsl/bs-json",
    "@ahrefs/bs-reactstrap",
    "re-formality"
